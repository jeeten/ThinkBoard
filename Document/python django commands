python manage.py makemigrations "appName"
python manage.py migrate "appName"
python manage.py sqlmigrate <appname> <migration no eg. 0001 or 0004>
python manage.py sqlmigrate <appname>
python manage.py makemigrations --name=<name_of_migration>
python manage.py migrate <name_of_app> <rollback_migration_name>
python manage.py migrate <name_of_app> zero #Removing all the migration from the particular app
python manage.py showmigrations user
python manage.py migrate --fake core zero #Clear the migration history (please note that core is the name of my app):

